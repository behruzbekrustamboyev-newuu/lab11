#include <iostream>
using namespace std;

int sum2D(int** m, int r, int c) {
    int sum = 0;
    for(int** row = m; row < m + r; row++) {
        for(int* col = *row; col < *row + c; col++) {
            sum += *col;
        }
    }
    return sum;
}

