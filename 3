#include <iostream>
using namespace std;

int countValid(int** arr, int size) {
    int count = 0;
    int** p = arr;

    for(int i = 0; i < size; i++, p++) {
        if(*p != nullptr)
            count++;
    }
    return count;
}

int main() {
    int a = 10, b = 20;
    int* arr[] = { &a, nullptr, &b, nullptr };

    cout << countValid(arr, 4);
    return 0;
}
