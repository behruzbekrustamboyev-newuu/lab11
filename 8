#include <iostream>
using namespace std;

void reverseArr(int** arr, int size) {
    int* start = *arr;
    int* end = *arr + size - 1;

    while (start < end) {
        int temp = *start;
        *start = *end;
        *end = temp;

        start++;
        end--;
    }
}

int main() {
    int arr[] = {1, 2, 3, 4, 5
